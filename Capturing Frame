import cv2

capture = cv2.VideoCapture(0)

while True:
    # capture frame
    ret, frame = capture.read()
    gray = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)
    #printing fps
    print(cv2.CAP_PROP_FPS)
    # Display result
    cv2.imshow('Camera', gray)
    if cv2.waitKey(1) & 0xFF == ord('q'):
        break
capture.release()
cv2.destroyAllWindows()
